CamAleatoria <- function(n,z){
x<- vector("numeric",0)
x[0] <- z
for(i in 1:n){
length(x) <- length(x) +1
moneda <- runif(1)
if(moneda<=0.5){
z <- z+1
x[i] <- z
} else {
z <- z-1
x[i] <- z
}
}
x
}
CamAleatoria(10,100)
CamAleatoria <- function(n,z){
x<- vector("numeric",1)
x[1] <- z
for(i in 2:(n+1)){
length(x) <- length(x) +1
moneda <- runif(1)
if(moneda<=0.5){
z <- z+1
x[i] <- z
} else {
z <- z-1
x[i] <- z
}
}
x
}
